<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="OrangeHRM Regression Suite"  verbose="1"> <!-- parallel="tests"
	thread-count="3" --> 

	<test name="OrangeHRM - LoginPage Test">
		<parameter name="browserName" value="chrome" />
		<classes>
			<class name="com.qa.oranges.tests.LoginPageTest" />

		</classes>

	</test>


	<test name="OrangeHRM - DashboardPage Test">
		<parameter name="browserName" value="Firefox" />
		<classes>
			<class name="com.qa.oranges.tests.DashboardPageTest" />

		</classes>

	</test>
	<test name="OrangeHRM - PIMpage Test">
		<parameter name="browserName" value="chrome" />
		<classes>
			<class name="com.qa.oranges.tests.PIMpageTest" />

		</classes>

	</test>



</suite>